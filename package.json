{
  "name": "one-file",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "dependencies": {
    "concurrently": "^4.0.1",
    "is-electron": "^2.1.0",
    "node-dbf": "^0.4.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "2.0.5",
    "wait-on": "^3.1.0",
    "xlsx": "^0.14.0"
  },
  "scripts": {
    "react-start": "set \"BROWSER=none\" && react-scripts start",
    "electron-start": "set \"ELECTRON_START_URL=http://127.0.0.1:3000\" && electron .",
    "start": "concurrently \"npm run react-start\" \"wait-on http://127.0.0.1:3000/ && npm run electron-start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^3.0.5"
  }
}
