{
  "name": "one-file",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "dependencies": {
    "async": "^2.6.1",
    "concurrently": "^4.0.1",
    "is-electron": "^2.1.0",
    "node-dbf": "^0.4.0",
    "node-sass": "^4.9.4",
    "object-formdata-convertor": "^1.2.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-block-ui": "^1.1.3",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-toastify": "^2.2.1",
    "redux": "^4.0.1",
    "redux-form": "^7.4.2",
    "redux-thunk": "^2.3.0",
    "wait-on": "^3.1.0",
    "xlsx-populate": "^1.17.0"
  },
  "scripts": {
    "react-start": "set \"BROWSER=none\" && react-scripts start",
    "electron-start": "set \"ELECTRON_START_URL=http://127.0.0.1:3000\" && electron .",
    "start": "concurrently \"npm run react-start\" \"wait-on http://127.0.0.1:3000/ && npm run electron-start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^3.0.5"
  }
